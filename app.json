{
  "expo": {
    "name": "CBSF",
    "description": "Chinese Bible Study Fellowship",
    "slug": "CBSFApp",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2018.03.30.0",
    "primaryColor": "#cccccc",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/images/icon-ios.png",
      "buildNumber": "1",
      "bundleIdentifier": "org.CBSF.bsfclass",
      "supportsTablet": true,
      "loadJSInBackgroundExperimental": true
    },
    "android": {
      "icon": "./assets/images/icon-android.png",
      "package": "org.cbsfappv1.bsfclass",
      "permissions": [
        "WAKE_LOCK"
      ],
      "versionCode": 11
    }
  }
}